{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "no-unused-vars": "warn",
        "no-undef": "warn",
        "comma-dangle": ["warn", "always-multiline"],
        "object-curly-spacing": ["error", "always"],
        "no-extra-semi": "warn",
        "prefer-const": "warn",
        "max-len": ["warn", { "code": 80 }],
        "no-duplicate-imports": "error"
    }
}
